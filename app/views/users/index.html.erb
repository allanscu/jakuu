<p id="notice"><%= notice %></p>

<div class="row">
	<div class="span12">
		<% @users.each do |user| %>
			<div class="row">
				<div class="span1">
			        <%= user.first_name %>
					<%= user.last_name %>
					<%= user.address_1 %>	
					<%= user.address_2 %>
					<%= user.city %>
					<%= user.state %>
					<%= user.zip_code %>
					<%= user.country %>
					<%= user.email %>
					<%= number_to_phone(user.phone, area_code: true) %>
			  	  	<%= link_to 'Show', user %>
					<%= link_to 'Edit', edit_user_path(user) %>
					<%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
				</div>
			</div>
		<% end %>
	</div>
</div>

<br />

<div class="row">
	<div class="span1">
		<div class="row">
			<div class="span1">
			    <div class="actions">
			      <%= link_to 'New User', new_user_path, class: "btn btn-default" %>
			    </div>
			</div>
		</div>
	</div>
</div>